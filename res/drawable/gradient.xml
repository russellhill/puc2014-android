<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item>  
	    <shape>
	        <gradient 
	            android:startColor="#4FB8A9"
	            android:endColor="#357A70"
	            android:type="linear"
	            android:angle="270"
	            android:centerColor="#25CCB4" />
	        <stroke
	            android:width="1dp"
	            android:color="#357A70" />
	    </shape>
	</item>
	<item>  
	    <shape>
	        <gradient 
	            android:startColor="#00666666"
	            android:endColor="#77666666"
	            android:type="radial"
	            android:gradientRadius="600"
	            android:centerColor="#00666666"
	            android:centerX="0.5"
	            android:centerY="0.5" />
	        <stroke
	            android:width="1dp"
	            android:color="#357A70" />
	    </shape>
	</item>
</layer-list>
